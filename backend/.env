# Supabase Configuration (for local development - update with your actual values)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Database Configuration (for direct connection - update with your values)
DB_HOST=localhost
DB_PORT=54322
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL_MODE=disable

# Server Configuration
PORT=8080
GIN_MODE=debug
JWT_SECRET=dev-jwt-secret-change-in-production

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# File Upload Configuration
MAX_UPLOAD_SIZE=10485760
UPLOAD_PATH=./uploads

# External API Keys (add your keys when needed)
OPENAI_API_KEY=
REPLICATE_API_TOKEN=

# Environment
ENVIRONMENT=development