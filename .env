# Google Gemini API Configuration
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Development Environment
NODE_ENV=development
DEBUG=true

# API Configuration
API_BASE_URL=http://localhost:3000
API_TIMEOUT=30000

# Database Configuration
DATABASE_URL=postgresql://localhost:5432/compozit_vision
MONGODB_URI=mongodb://localhost:27017/compozit_vision

# AWS Configuration (for production)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET_NAME=compozit-vision-assets

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=7d

# External Services
STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Analytics
FIREBASE_PROJECT_ID=
FIREBASE_PRIVATE_KEY=
FIREBASE_CLIENT_EMAIL=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Image Processing
MAX_IMAGE_SIZE_MB=50
SUPPORTED_IMAGE_FORMATS=jpeg,jpg,png,heic,webp,tiff

# AI Processing
AI_PROCESSING_TIMEOUT=120000
MAX_CONCURRENT_AI_REQUESTS=10
AI_CACHE_TTL=3600