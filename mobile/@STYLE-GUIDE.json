{
  "designSystem": {
    "name": "Compozit Vision Design System",
    "version": "1.0.0",
    "description": "Clean, modern design system for interior design app"
  },
  "tokens": {
    "color": {
      "bgApp": "#F4F0E8",
      "bgSecondary": "#EDE5D8",
      "surface": "#FFFFFF", 
      "textPrimary": "#1F1D1A",
      "textSecondary": "#6B5F53",
      "textMuted": "#A89C91",
      "textInverse": "#FFFFFF",
      "borderSoft": "#D8CFC0",
      "borderWarm": "#C2B4A2",
      "brand": "#C8955E",
      "brandLight": "#E0B583",
      "brandDark": "#A67844",
      "accent": "#1A1816",
      "accentSoft": "#4A453F",
      "warm": "#E0B583",
      "warmDark": "#C8955E",
      "scrim": "rgba(26,24,22,0.55)",
      "scrimHeavy": "rgba(26,24,22,0.75)",
      "success": "#6BA555",
      "error": "#D86B4A",
      "warning": "#EAB742"
    },
    "radius": {
      "sm": 8,
      "md": 12,
      "lg": 16,
      "xl": 24,
      "pill": 999
    },
    "shadow": {
      "e1": {
        "shadowColor": "#000",
        "shadowOpacity": 0.06,
        "shadowRadius": 3,
        "shadowOffset": { "width": 0, "height": 1 },
        "elevation": 2
      },
      "e2": {
        "shadowColor": "#000", 
        "shadowOpacity": 0.08,
        "shadowRadius": 12,
        "shadowOffset": { "width": 0, "height": 4 },
        "elevation": 4
      },
      "e3": {
        "shadowColor": "#000",
        "shadowOpacity": 0.12,
        "shadowRadius": 20,
        "shadowOffset": { "width": 0, "height": 8 },
        "elevation": 8
      }
    },
    "typography": {
      "display": {
        "fontSize": 32,
        "lineHeight": 40,
        "fontWeight": "700"
      },
      "h1": {
        "fontSize": 28,
        "lineHeight": 36,
        "fontWeight": "600"
      },
      "h2": {
        "fontSize": 22,
        "lineHeight": 28,
        "fontWeight": "600"
      },
      "h3": {
        "fontSize": 18,
        "lineHeight": 24,
        "fontWeight": "500"
      },
      "body": {
        "fontSize": 16,
        "lineHeight": 22,
        "fontWeight": "400"
      },
      "small": {
        "fontSize": 14,
        "lineHeight": 20,
        "fontWeight": "400"
      },
      "caption": {
        "fontSize": 12,
        "lineHeight": 16,
        "fontWeight": "400"
      }
    },
    "spacing": {
      "xs": 4,
      "sm": 8,
      "md": 12,
      "lg": 16,
      "xl": 24,
      "xxl": 32,
      "xxxl": 48
    }
  },
  "components": {
    "glassMorphism": {
      "light": {
        "backgroundColor": "rgba(255, 255, 255, 0.1)",
        "borderWidth": 1,
        "borderColor": "rgba(255, 255, 255, 0.2)",
        "backdropBlur": 12
      },
      "dark": {
        "backgroundColor": "rgba(0, 0, 0, 0.3)",
        "borderWidth": 1,
        "borderColor": "rgba(255, 255, 255, 0.1)",
        "backdropBlur": 12
      },
      "card": {
        "backgroundColor": "rgba(255, 255, 255, 0.08)",
        "borderWidth": 1,
        "borderColor": "rgba(255, 255, 255, 0.18)",
        "backdropBlur": 16
      }
    },
    "button": {
      "primary": {
        "gradientColors": ["#C8955E", "#A67844"],
        "gradientStart": { "x": 0, "y": 0 },
        "gradientEnd": { "x": 1, "y": 1 },
        "borderRadius": 28,
        "height": 56,
        "pressedOpacity": 0.9,
        "shadowColor": "#1A1816",
        "shadowOpacity": 0.25,
        "shadowRadius": 16,
        "shadowOffset": { "width": 0, "height": 6 },
        "elevation": 8,
        "textColor": "#FFFFFF",
        "fontSize": 18,
        "fontWeight": "700"
      },
      "secondary": {
        "gradientColors": ["#EDE5D8", "#D8CFC0"],
        "gradientStart": { "x": 0, "y": 0 },
        "gradientEnd": { "x": 1, "y": 1 },
        "color": "#1F1D1A",
        "borderRadius": 999,
        "height": 52,
        "pressedOpacity": 0.85,
        "borderWidth": 1.5,
        "borderColor": "#C2B4A2",
        "shadowColor": "#1A1816",
        "shadowOpacity": 0.15,
        "shadowRadius": 8,
        "shadowOffset": { "width": 0, "height": 3 },
        "elevation": 4
      },
      "ghost": {
        "backgroundColor": "transparent",
        "color": "#2D2B28",
        "borderWidth": 1,
        "borderColor": "#D4C7B5",
        "borderRadius": 999,
        "height": 52,
        "pressedOpacity": 0.8,
        "hoverBackgroundColor": "#F5F1E8"
      },
      "accent": {
        "gradientColors": ["#1A1816", "#4A453F"],
        "gradientStart": { "x": 0, "y": 0 },
        "gradientEnd": { "x": 1, "y": 1 },
        "color": "#FFFFFF",
        "borderRadius": 999,
        "height": 52,
        "pressedOpacity": 0.85,
        "shadowColor": "#1A1816",
        "shadowOpacity": 0.3,
        "shadowRadius": 12,
        "shadowOffset": { "width": 0, "height": 5 },
        "elevation": 6
      }
    },
    "input": {
      "backgroundColor": "#FFFFFF",
      "borderWidth": 1,
      "borderColor": "#E8E2D8",
      "borderRadius": 12,
      "height": 48,
      "paddingHorizontal": 16,
      "focusBorderColor": "#C9A98C"
    },
    "card": {
      "backgroundColor": "#FFFFFF",
      "borderRadius": 16,
      "padding": 24,
      "shadow": "e2",
      "borderWidth": 1,
      "borderColor": "#E8E2D8"
    },
    "onboarding": {
      "backgroundGradient": {
        "colors": ["#F4F0E8", "#EDE5D8", "#E6DDD1"],
        "start": { "x": 0, "y": 0 },
        "end": { "x": 1, "y": 1 }
      },
      "badge": {
        "backgroundColor": "rgba(255, 255, 255, 0.25)",
        "borderRadius": 24,
        "paddingHorizontal": 20,
        "paddingVertical": 10,
        "borderWidth": 1.5,
        "borderColor": "rgba(255, 255, 255, 0.4)",
        "shadowColor": "#1A1816",
        "shadowOpacity": 0.15,
        "shadowRadius": 8,
        "shadowOffset": { "width": 0, "height": 3 },
        "elevation": 4
      },
      "featureCard": {
        "backgroundColor": "rgba(255, 255, 255, 0.15)",
        "borderRadius": 20,
        "padding": 20,
        "borderWidth": 1.5,
        "borderColor": "rgba(255, 255, 255, 0.3)",
        "backdropBlur": 20,
        "shadowColor": "#1A1816",
        "shadowOpacity": 0.2,
        "shadowRadius": 12,
        "shadowOffset": { "width": 0, "height": 4 },
        "elevation": 6
      },
      "featureIcon": {
        "size": 48,
        "gradientColors": ["rgba(200, 149, 94, 0.25)", "rgba(166, 120, 68, 0.35)"],
        "gradientStart": { "x": 0, "y": 0 },
        "gradientEnd": { "x": 1, "y": 1 },
        "borderRadius": 16,
        "borderWidth": 1,
        "borderColor": "rgba(200, 149, 94, 0.4)"
      },
      "titleContainer": {
        "backgroundColor": "rgba(26, 24, 22, 0.4)",
        "borderRadius": 16,
        "paddingHorizontal": 24,
        "paddingVertical": 16,
        "backdropBlur": 12,
        "borderWidth": 1,
        "borderColor": "rgba(255, 255, 255, 0.15)"
      },
      "progressBar": {
        "backgroundColor": "rgba(255, 255, 255, 0.3)",
        "fillGradient": ["#C8955E", "#A67844"],
        "height": 6,
        "borderRadius": 3
      }
    },
    "animation": {
      "duration": {
        "fast": 200,
        "normal": 300,
        "slow": 500,
        "verySlow": 800
      },
      "easing": {
        "default": "ease-in-out",
        "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
      }
    }
  }
}